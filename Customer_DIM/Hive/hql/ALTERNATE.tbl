use customer;
create EXTERNAL table ALTERNATE
(
INTERNAL_ID   INT,
ALT_ID  STRING,
ALT_LONG_NM  STRING,
ALT_SHORT_NM  STRING,
EFFECTIVE_DT  TIMESTAMP,
EXPIRY_DT  TIMESTAMP,
VERIFY_STS  INT,
VERIFIED_BY  STRING,
VERIFIED_DT  TIMESTAMP,
CHANGE_BY   STRING,
CHANGE_DT   TIMESTAMP
)
PARTITIONED BY (LOAD_DT STRING)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '\t' 
STORED AS TEXTFILE
LOCATION '/user/hduser/Project/Hive/ALTERNATE';
